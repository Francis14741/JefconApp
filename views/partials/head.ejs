    <!-- Page-specific small tweaks -->
    <style>
      /* Map container height */
      #map {
        height: 500px;
        width: 100%;
      }

      /* Accordion animation */
      .accordion input[type="radio"] { display: none; }
      .accordion .content { display: none; padding: 1rem; }
      .accordion input[type="radio"]:checked ~ .content {
        display: block;
        animation: fadeIn 0.3s ease-in-out;
      }
      @keyframes fadeIn {
        from { opacity: 0; }
        to { opacity: 1; }
      }
    </style>

