              <script>
                var lat = 6.207674;
                var lng = 6.69759;

                // Initialize map
                var map = L.map('map').setView([lat, lng], 18);
              
                // Add OpenStreetMap tiles
                L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {
                  maxZoom: 19,
                }).addTo(map);

                // Add marker with button in popup
