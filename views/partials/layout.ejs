                  $('#streetViewModal').modal('show');
                }

                var contactMap = L.map('contact-map').setView([6.207674, 6.69759], 18);
                L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', { maxZoom: 19 }).addTo(contactMap);
                L.marker([6.207674, 6.69759]).addTo(contactMap).bindPopup("Our Office").openPopup();

                // Footer Map
                var footerMap = L.map('footer-map', {
                  center: [6.207674, 6.69759],
                  zoom: 15,
                  dragging: false,
                  scrollWheelZoom: false,
                  doubleClickZoom: false,
                  zoomControl: false,
                  touchZoom: false
                });
                L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', { maxZoom: 19 }).addTo(footerMap);
                L.marker([6.207674, 6.69759]).addTo(footerMap);
