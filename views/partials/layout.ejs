                  <div id="fixed-address">
                    <strong><em>No. 11 Engr. John Chiegbuniwe Drive,<br>Asaba, Delta State, Nigeria</em></strong><br><br>
                    <button class="btn btn-sm btn-primary" onclick="openStreetView()">View in Street View</button>
                  </div>
                `;

                L.marker([lat, lng]).addTo(map).bindPopup(popupContent).openPopup();

                // Function to open Street View inside modal
                function openStreetView() {
                  var streetViewURL = "https://www.google.com/maps/embed?pb=!4v1662388809655!6m8!1m7!1sCAoSLEFGMVFpcE5jc0x2TVVuUWJ0Y2syZ1A3QlZ3X0tZcDhZZlZkSEtPNVlyWm9S!2m2!1d6.207674!2d6.69759!3f90!4f0!5f0.7820865974627469";
